<h1 class='homepage-title'>Find Your Elected Official</h1>
<form>
  <div class="row">
    <div class="main form">
      <label class='address'>Address
        <input type="text" placeholder="123 Main St">
      </label>

      <label class='city'>City
        <input type="text" placeholder="Boston">
      </label>

      <label class='state'>State
        <input type="text" placeholder="MA">
      </label>

      <label class='zip'>Zip
        <input type="text" placeholder="02110">
      </label>
    </div>
    <script type="text/javascript" src="tool.votinginfoproject.org/app.js"></script>
    <div id="_vit"></div>
    <script type="text/javascript">vit.load({});</script>
    <div>
      <input type='submit' id='submit button' value='Submit'>
    </div>
  </div>
</form>
